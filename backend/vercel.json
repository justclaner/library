{
    "version": 2,
    "outputDirectory":"build",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)","dest": "/"},
      { "src":"/books", "dest":"/"}
    ],
    "redirects": [
      {"source":"/", "destination":"/books"}
    ]
  }